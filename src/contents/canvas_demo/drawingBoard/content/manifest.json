{
  "name": "Grandstream GRP Click2Dial",
  "version": "1.0.1.3",
  "description": "Click2Dial extension for GRP260X. This extension works with V1.0.5.11 or upper version GRP260X.",
  "manifest_version": 3,
  "action":
  {
    "default_title": "GRP Click2Dial",
    "default_icon": "quicall/assets/logo.png"
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "icons": {
    "16": "quicall/assets/logo_16x16.png",
    "48": "quicall/assets/logo_48x48.png",
    "128": "quicall/assets/logo_128x128.png"
  },
  "content_scripts":
  [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts/contentScript.js"
      ],
      "css": [
        "contentScripts/contentScript.css"
      ],
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["contentScripts/injectScript.js","contentScripts/*"],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "management",
    "notifications"
  ]
}
